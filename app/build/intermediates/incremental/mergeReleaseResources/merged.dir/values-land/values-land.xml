<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <bool name="is_landscape">true</bool>
    <dimen name="abc_action_bar_default_height_material">48dp</dimen>
    <dimen name="abc_text_size_subtitle_material_toolbar">12dp</dimen>
    <dimen name="abc_text_size_title_material_toolbar">14dp</dimen>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="contact_delete_dialog_text_buttons_height">50dp</dimen>
    <dimen name="contact_delete_dialog_text_height">90dp</dimen>
    <dimen name="contact_delete_dialog_width">350dp</dimen>
    <dimen name="contact_edit_name_text_size">16sp</dimen>
    <dimen name="contact_edit_other_text_size">14sp</dimen>
    <dimen name="contact_initial_diameter">40dp</dimen>
    <dimen name="dialer_button_letters_offset">-5dp</dimen>
    <dimen name="margin_large">8dp</dimen>
    <dimen name="margin_normal">8dp</dimen>
    <dimen name="margin_xlarge">16dp</dimen>
    <dimen name="persistent_control_radius">32dp</dimen>
    <dimen name="persistent_control_stroke_width">2dp</dimen>
    <dimen name="sync_button_text_size">18sp</dimen>
    <dimen name="tab_height">56dp</dimen>
    <fraction name="video_controls_alpha">0.7</fraction>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AvayaSimpleAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:colorAccent">@color/colorAccent</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textColorPrimary">#ff535353</item>
    </style>
    <style name="Body1">
        <item name="android:fontFamily">sans-serif-regular</item>
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="CallStatusLower" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textAlignment">center</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="CallStatusUpper" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textAlignment">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Display1">
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textSize" ns1:ignore="SpUsage">34dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="Display3">
        <item name="android:fontFamily">sans-serif-regular</item>
        <item name="android:textSize" ns1:ignore="SpUsage">56dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="ErrorMessageActivityTheme" parent="Theme.Transparent">
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="Headline">
        <item name="android:fontFamily">sans-serif-regular</item>
        <item name="android:textSize" ns1:ignore="SpUsage">24dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>
    <style name="StatusFont" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="SubStatusFont" parent="StatusFont">
        <item name="android:textColor">@color/colorSubStatus</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Subheading">
        <item name="android:fontFamily">sans-serif-regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="Theme.Transparent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/dimBackgroundColor</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>
    <style name="Title">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize" ns1:ignore="SpUsage">20dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="Base.Widget.Design.TabLayout">
    <item name="tabGravity">center</item>
    <item name="tabMode">fixed</item>
  </style>
    <style name="active_call_dialpad">
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="active_call_dialpad_button" parent="active_call_dialpad">
        <item name="android:textSize">33sp</item>
    </style>
    <style name="active_call_dialpad_dialer_text">
        <item name="android:textColor">@color/colorControlStroke</item>
        <item name="android:textSize">33sp</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="active_call_dialpad_letters">
        <item name="android:textColor">@color/colorSubGrey</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="call_features_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">71dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">16dp</item>
        <item name="android:drawablePadding">16dp</item>
    </style>
    <style name="call_features_title_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">72dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="contact_type_spinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/layout_on_press</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxWidth">300dp</item>
        <item name="android:minWidth">130dp</item>
        <item name="android:paddingStart">@dimen/margin_large</item>
        <item name="android:paddingEnd">@dimen/margin_large</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">22sp</item>
    </style>
    <style name="dialog_call_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">71dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/secondary</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">16dp</item>
        <item name="android:drawablePadding">16dp</item>
    </style>
    <style name="dialog_separator_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:background">@color/colorDialpadSepratorLine</item>
    </style>
    <style name="favorites_title_style">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@drawable/contact_search_bg</item>
    </style>
    <style name="initials_contact_detailes_text_style" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textSize">22sp</item>
    </style>
    <style name="new_call_dialpad_dialer_text">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">56sp</item>
        <item name="android:fontFamily">sans-serif-thin</item>
    </style>
    <style name="new_call_dialpad_dialer_text_name">
        <item name="android:textColor">@color/colorSubGrey</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="phone_type_spinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/layout_on_press</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxWidth">300dp</item>
        <item name="android:minWidth">130dp</item>
        <item name="android:paddingStart">@dimen/margin_large</item>
        <item name="android:paddingEnd">@dimen/margin_large</item>
        <item name="android:textColor">@color/colorActiveCallBG</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="searchTextHiglight">
        <item name="android:textColor">@color/holo_blue</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="tutorialText">
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="tutorialText.Large">
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">28sp</item>
    </style>
    <style name="tutorial_description_text_style">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize" ns1:ignore="SpUsage">19dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:gravity">center</item>
    </style>
</resources>